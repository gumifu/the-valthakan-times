# Beehiiv API Configuration
# Get your API key from: https://www.beehiiv.com/settings/api
BEEHIIV_API_KEY=your_api_key_here

# Beehiiv Publication ID (without the "pub_" prefix)
# You can find this in your Beehiiv dashboard
BEEHIIV_PUBLICATION_ID=your_publication_id_here

# Instagram Graph API Configuration
# Get your access token from: https://developers.facebook.com/docs/instagram-basic-display-api
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token_here
INSTAGRAM_USER_ID=your_instagram_user_id_here
